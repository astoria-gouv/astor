{
  "dashboard": {
    "id": null,
    "title": "Astor Currency System",
    "tags": ["astor", "currency", "financial"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(astor_http_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Requests per second"
          }
        ]
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(astor_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(astor_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(astor_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ]
      },
      {
        "id": 3,
        "title": "Transactions",
        "type": "stat",
        "targets": [
          {
            "expr": "astor_transactions_total",
            "legendFormat": "Total Transactions"
          }
        ]
      },
      {
        "id": 4,
        "title": "Currency Issued",
        "type": "stat",
        "targets": [
          {
            "expr": "astor_currency_issued_total",
            "legendFormat": "Total ASTOR Issued"
          }
        ]
      },
      {
        "id": 5,
        "title": "Active Accounts",
        "type": "stat",
        "targets": [
          {
            "expr": "astor_active_accounts",
            "legendFormat": "Active Accounts"
          }
        ]
      },
      {
        "id": 6,
        "title": "Security Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(astor_failed_logins_total[5m])",
            "legendFormat": "Failed Logins/sec"
          },
          {
            "expr": "rate(astor_security_violations_total[5m])",
            "legendFormat": "Security Violations/sec"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
}
